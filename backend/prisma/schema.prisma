datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  previewFeatures = ["nativeTypes"]
  provider        = "prisma-client-js"
}

model user {
  id         String @id
  lastName   String @db.Text
  firstName  String @db.Text
  email      String @db.Text
  userStatus String @db.Text
}

